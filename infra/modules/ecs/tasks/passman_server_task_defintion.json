[
    {
      "name": "dev-app",
      "image": "${image}",
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "networkMode": "awsvpc",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "${log_group}",
          "awslogs-region": "${region}",
          "awslogs-stream-prefix": "passman-server"
        }
      },
      "environment": [
        {
          "name": "MYSQL_ENDPOINT",
          "value": "${mysql_endpoint}"
        },
        {
          "name": "AWS_REGION",
          "value": "${region}"
        },
        {
          "name": "PORT",
          "value": "80"
        }
      ]
    }
  ]